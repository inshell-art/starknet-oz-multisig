{
  "network": "mainnet",
  "chain_id": "SN_MAIN",
  "rpc_allowlist": [
    "https://starknet-mainnet.g.alchemy.com/v2/<API_KEY>",
    "https://starknet-mainnet.infura.io/v3/<API_KEY>"
  ],
  "signer_aliases": {
    "MAINNET_DEPLOY_SW_A": {
      "type": "keystore",
      "account_json_env": "MAINNET_DEPLOY_ACCOUNT_JSON",
      "keystore_env": "MAINNET_DEPLOY_KEYSTORE_JSON"
    },
    "MAINNET_GOV_SW_A": {
      "type": "keystore",
      "account_json_env": "MAINNET_GOV_ACCOUNT_JSON",
      "keystore_env": "MAINNET_GOV_KEYSTORE_JSON"
    },
    "MAINNET_GOV_HW_B": {
      "type": "ledger"
    },
    "MAINNET_TREASURY_SW_A": {
      "type": "keystore",
      "account_json_env": "MAINNET_TREASURY_ACCOUNT_JSON",
      "keystore_env": "MAINNET_TREASURY_KEYSTORE_JSON"
    },
    "MAINNET_TREASURY_HW_B": {
      "type": "ledger"
    }
  },
  "lanes": {
    "lane0_observe": {
      "writes": false,
      "allowed_ops": [
        "read",
        "snapshot",
        "diff",
        "simulate"
      ]
    },
    "lane1_plan": {
      "writes": false,
      "allowed_ops": [
        "build",
        "compile",
        "derive_intent",
        "checks_readonly",
        "simulate"
      ]
    },
    "lane2_deploy": {
      "writes": true,
      "allowed_signers": [
        "MAINNET_DEPLOY_SW_A"
      ],
      "allowed_ops": [
        "declare",
        "deploy"
      ],
      "required_checks": [
        "chain_id",
        "rpc_allowlist",
        "signer_allowlist",
        "class_hash",
        "post_deploy_snapshot"
      ],
      "fee_policy": {
        "max_fee_native": "<SET_ME>",
        "require_fee_confirmation": true
      },
      "gates": {
        "require_sepolia_rehearsal_proof": true
      }
    },
    "lane3_handoff_lockdown": {
      "writes": true,
      "allowed_signers": [
        "MAINNET_DEPLOY_SW_A",
        "MAINNET_GOV_SW_A",
        "MAINNET_GOV_HW_B"
      ],
      "allowed_ops": [
        "grant_role",
        "revoke_role",
        "transfer_ownership",
        "accept_ownership",
        "multisig_submit",
        "multisig_confirm",
        "multisig_execute"
      ],
      "required_checks": [
        "chain_id",
        "rpc_allowlist",
        "signer_allowlist",
        "target_identity",
        "preconditions",
        "simulate",
        "postconditions"
      ],
      "gates": {
        "require_sepolia_rehearsal_proof": true
      }
    },
    "lane5_govern": {
      "writes": true,
      "allowed_signers": [
        "MAINNET_GOV_SW_A",
        "MAINNET_GOV_HW_B",
        "MAINNET_TREASURY_SW_A",
        "MAINNET_TREASURY_HW_B"
      ],
      "allowed_ops": [
        "multisig_submit",
        "multisig_confirm",
        "multisig_execute"
      ],
      "required_checks": [
        "chain_id",
        "rpc_allowlist",
        "signer_allowlist",
        "target_identity",
        "preconditions",
        "simulate",
        "postconditions"
      ],
      "multisig_rules": {
        "gov": {
          "quorum": 2,
          "signers": [
            "MAINNET_GOV_SW_A",
            "MAINNET_GOV_HW_B"
          ]
        },
        "treasury": {
          "quorum": 2,
          "signers": [
            "MAINNET_TREASURY_SW_A",
            "MAINNET_TREASURY_HW_B"
          ]
        }
      },
      "gates": {
        "require_sepolia_rehearsal_proof": true
      }
    }
  }
}